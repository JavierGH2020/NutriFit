{
  "kind": "collectionType",
  "collectionName": "alimentos",
  "info": {
    "singularName": "alimento",
    "pluralName": "alimentos",
    "displayName": "alimento",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "nombre": {
      "type": "string",
      "required": true
    },
    "calorias": {
      "type": "biginteger"
    },
    "proteinas": {
      "type": "integer"
    },
    "carbohidratos": {
      "type": "integer"
    },
    "grasas": {
      "type": "integer"
    },
    "tipo": {
      "type": "enumeration",
      "enum": [
        "desayuno",
        "almuerzo",
        "cena",
        "snack"
      ]
    },
    "historials": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::historial.historial",
      "mappedBy": "alimento"
    },
    "fecha": {
      "type": "date"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "alimentos"
    }
  }
}
