{
  "kind": "collectionType",
  "collectionName": "usuarios",
  "info": {
    "singularName": "usuario",
    "pluralName": "usuarios",
    "displayName": "DatoUsuario",
    "description": "Datos adicionales del usuario"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "edad": {
      "type": "integer",
      "required": true
    },
    "genero": {
      "type": "enumeration",
      "enum": [
        "hombre",
        "mujer"
      ],
      "required": true
    },
    "peso": {
      "type": "decimal",
      "required": true
    },
    "altura": {
      "type": "integer",
      "required": true
    },
    "nivelActividad": {
      "type": "enumeration",
      "enum": [
        "bajo",
        "medio",
        "alto"
      ],
      "required": true
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "dato_usuarios"
    }
  }
}
